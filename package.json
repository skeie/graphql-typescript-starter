{
  "name": "typescript-graphql",
  "version": "1.0.0",
  "engines": {
    "node": ">= 10",
    "npm": ">= 5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/skeie/graphql-typescript-starter"
  },
  "scripts": {
    "test": "jest",
    "start": "nodemon",
    "build-ts": "tsc -p .",
    "dev": "nodemon",
    "migrate": "db-migrate --config config/database.json",
    "migrate:up": "db-migrate up --config config/database.json",
    "heroku-postbuild": "yarn build-ts && yarn migrate:up",
    "migrate:up:prod": "db-migrate up --config config/database.json",
    "migrate:down": "db-migrate down --config config/database.json",
    "migrate:down:test": "NODE_ENV=test db-migrate down --config config/database.json --env test",
    "migrate:up:test": "NODE_ENV=test db-migrate up --config config/database.json --env test",
    "migrate:test": "db-migrate --config config/database.json --env test",
    "migrate:prod": "db-migrate --config config/database.json --env production",
    "lint": "node_modules/.bin/tslint ./src/**/*.ts",
    "lint:fix": "node_modules/.bin/tslint ./src/**/*.ts --fix",
    "resetdb": "dropdb mind_pump && createdb mind_pump && yarn migrate:up && dropdb mind_pump_test && createdb mind_pump_test && yarn migrate:up:test",
    "graphql:typegen": "gql-gen --config codegen.yml"
  },
  "dependencies": {
    "apn": "2.2.0",
    "apollo-datasource": "0.6.3",
    "apollo-datasource-rest": "0.6.4",
    "apollo-server": "2.9.3",
    "apollo-server-express": "2.9.3",
    "bcrypt": "3.0.6",
    "body-parser": "1.19.0",
    "camelcase": "5.3.1",
    "core-js": "3.2.1",
    "db-migrate": "0.11.6",
    "dotenv": "8.1.0",
    "express": "4.17.1",
    "graphql": "14.5.4",
    "heroku-logger": "0.3.3",
    "jsonwebtoken": "8.5.1",
    "morgan": "1.9.1",
    "pg-monitor": "1.2.1",
    "pg-promise": "9.1.2",
    "ts-node": "8.3.0"
  },
  "devDependencies": {
    "@types/bcrypt": "3.0.0",
    "@types/body-parser": "1.17.1",
    "@types/express": "4.17.1",
    "@types/graphql": "14.2.3",
    "@types/jest": "24.0.18",
    "@types/jsonwebtoken": "8.3.4",
    "@types/morgan": "1.7.37",
    "@types/node": "10.14.17",
    "apollo-server-core": "2.9.3",
    "apollo-server-testing": "2.9.3",
    "db-migrate-pg": "1.0.0",
    "graphql-code-generator": "0.18.2",
    "graphql-codegen-introspection": "0.18.2",
    "graphql-codegen-typescript-common": "0.18.2",
    "graphql-codegen-typescript-resolvers": "0.18.2",
    "graphql-codegen-typescript-server": "0.18.2",
    "jest": "24.9.0",
    "node-fetch": "2.6.0",
    "nodemon": "1.19.1",
    "prettier": "1.18.2",
    "ts-jest": "24.0.2",
    "tslint": "5.19.0",
    "tslint-config-airbnb": "5.11.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.0.1",
    "typescript": "3.6.2"
  },
  "jest": {
    "globalSetup": "<rootDir>test/jestsetup.js"
  }
}
